


{% for i,product in products %}


<!-- start single product -->
<div class="lg-product-wrapper colum-2 two-colum-parent-product col-lg-6" id="product_at_index_{{ i }}">
    <div class="inner">
        <div class="lg-left-content">
            <a href="../pages/product-details.html" >
                {% if product.images|length > 1 %}
                    <!-- Swiper -->
                    <div class="swiper effect-creative-swiper rounded"  >
                        <div class="swiper-wrapper">
                            {% for image in product.images %}
                                <div class="swiper-slide">
                                    <a href="#"
                                       onclick="document.getElementById('form_details_{{ i }}').submit(); return false;">
                                        <img src="{{ asset(image.path)|default("Not available") }}"
                                             alt="NFT_portfolio"
                                             class="fixedImagesSize4owner"/>
                                    </a>
                                </div>
                            {% endfor %}
                        </div>
                        <div class="swiper-pagination"></div>
                    </div>
                {% else %}
                    <div >
                        <a href="#"
                           onclick="document.getElementById('form_details_{{ i }}').submit(); return false;">
                            <img src="{{ asset(product.images[0].path) }}"
                                 alt="NFT_portfolio"
                                 class="fixedImagesSize4owner"
                            >
                        </a>
                    </div>
                {% endif %}
            </a>
            <div class="read-content">
                <div class="product-share-wrapper">
                    <!-- all bids -->
                    <div class="profile-share">
                        <a
                                href="author.html"
                                class="avatar"
                                data-tooltip="Farik Shaikh"
                                tabindex="0"
                        ><img
                                    src="../../assets/images/client/client-1.png"
                                    alt="Nft_Profile"
                            /></a>
                        <a
                                href="author.html"
                                class="avatar"
                                data-tooltip="Shaikh"
                                tabindex="0"
                        ><img
                                    src="../../assets/images/client/client-6.png"
                                    alt="Nft_Profile"
                            /></a>
                        <a
                                href="author.html"
                                class="avatar"
                                data-tooltip="Farik"
                                tabindex="0"
                        ><img
                                    src="../../assets/images/client/client-8.png"
                                    alt="Nft_Profile"
                            /></a>
                        <a class="more-author-text" href="#" tabindex="0"
                        >15+ Place Bit.</a
                        >
                    </div>
                    <!-- all bids End-->
                    <div class="last-bid">0.244wETH</div>
                </div>
                <a href="product-details.html">
                    <h6 class="title">New Dod Glitter Worlds</h6>
                </a>
                <span class="latest-bid">Highest bid 1/20</span>
                <div class="share-wrapper d-flex">
                    <!-- react area -->
                    <div class="react-area mr--15">
                        <svg
                                viewBox="0 0 17 16"
                                fill="none"
                                width="16"
                                height="16"
                                class="sc-bdnxRM sc-hKFxyN kBvkOu"
                        >
                            <path
                                    d="M8.2112 14L12.1056 9.69231L14.1853 7.39185C15.2497 6.21455 15.3683 4.46116 14.4723 3.15121V3.15121C13.3207 1.46757 10.9637 1.15351 9.41139 2.47685L8.2112 3.5L6.95566 2.42966C5.40738 1.10976 3.06841 1.3603 1.83482 2.97819V2.97819C0.777858 4.36443 0.885104 6.31329 2.08779 7.57518L8.2112 14Z"
                                    stroke="currentColor"
                                    stroke-width="2"
                            ></path>
                        </svg>
                        <span class="number">65</span>
                    </div>
                    <!-- end -->
                    <!-- share area -->
                    <div class="share-btn share-btn-activation dropdown">
                        <button
                                class="icon"
                                data-bs-toggle="dropdown"
                                aria-expanded="false"
                                tabindex="0"
                        >
                            <svg
                                    viewBox="0 0 14 4"
                                    fill="none"
                                    width="16"
                                    height="16"
                                    class="sc-bdnxRM sc-hKFxyN hOiKLt"
                            >
                                <path
                                        fill-rule="evenodd"
                                        clip-rule="evenodd"
                                        d="M3.5 2C3.5 2.82843 2.82843 3.5 2 3.5C1.17157 3.5 0.5 2.82843 0.5 2C0.5 1.17157 1.17157 0.5 2 0.5C2.82843 0.5 3.5 1.17157 3.5 2ZM8.5 2C8.5 2.82843 7.82843 3.5 7 3.5C6.17157 3.5 5.5 2.82843 5.5 2C5.5 1.17157 6.17157 0.5 7 0.5C7.82843 0.5 8.5 1.17157 8.5 2ZM11.999 3.5C12.8274 3.5 13.499 2.82843 13.499 2C13.499 1.17157 12.8274 0.5 11.999 0.5C11.1706 0.5 10.499 1.17157 10.499 2C10.499 2.82843 11.1706 3.5 11.999 3.5Z"
                                        fill="currentColor"
                                ></path>
                            </svg>
                        </button>
                        <div
                                class="share-btn-setting dropdown-menu dropdown-menu-end"
                        >
                            <button
                                    type="button"
                                    class="btn-setting-text share-text"
                                    data-bs-toggle="modal"
                                    data-bs-target="#shareModal"
                                    tabindex="0"
                            >
                                Share
                            </button>
                            <button
                                    type="button"
                                    class="btn-setting-text report-text"
                                    data-bs-toggle="modal"
                                    data-bs-target="#reportModal"
                                    tabindex="0"
                            >
                                Report
                            </button>
                        </div>
                    </div>
                    <!-- sharea End -->
                </div>
            </div>
        </div>
        <button
                type="button"
                class="btn btn-primary-alta"
                data-bs-toggle="modal"
                data-bs-target="#placebidModal"
        >
            Place a Bid
        </button>
    </div>
</div>
<!-- End single product -->


{% endfor %}













