       id="style-rtl">
 <link href="{{ asset('assetsAdmin/assets/css/theme.min.css') }}" type="text/css" rel="stylesheet"
       id="style-default">
 <link href="{{ asset('assetsAdmin/assets/css/user-rtl.min.css') }}" type="text/css" rel="stylesheet"
       id="user-style-rtl">
 <link href="{{ asset('assetsAdmin/assets/css/user.min.css') }}" type="text/css" rel="stylesheet"
       id="user-style-default">
