<input type="hidden" id="currentPage" value={{ current_page }}>
<input type="hidden" id="previousPage" value={{ previous_page }}>

{% for i,product in products %}
    <!-- start single product -->
    <div
            class="col-lg-4 col-md-6 col-sm-12"
            data-sal="slide-up"
            data-sal-delay="{{ 200*i +100 }}"
            data-sal-duration="800"
    >
        <div class="product-style-one no-overlay with-placeBid">

            <div class="card-thumbnail">
                <a href="product-details.html.twig">
{#                                        <img#}
{#                                                src="{{ asset('/assets/images/portfolio/portfolio-02.jpg') }}"#}
{#                                                alt="NFT_portfolio"#}
{#                                                width="225"#}
{#                                                height="225"#}
{#                                        />#}
                    <!-- Swiper -->
                    {#                    <div class="swiper effect-creative-swiper rounded"> #}
                    {#                        <div class="swiper-wrapper"> #}
                    {#                            {% for image in product.images %} #}
                    {#                                <div class="swiper-slide"> #}
                    {#                                    <img src="{{ asset(image.path)|default("Not available") }}" #}
                    {#                                         alt="NFT_portfolio" #}
                    {#                                         width="225" #}
                    {#                                         height="225" #}
                    {#                                    /> #}
                    {#                                </div> #}
                    {#                            {% endfor %} #}
                    {#                        </div> #}
                    {#                        <div class="swiper-pagination"></div> #}
                    {#                    </div> #}

                    <!-- Swiper -->
                                        <div class="swiper rounded effect-creative-swiper" id="product_images_{{ i+1 }}">
                                            <div class="swiper-wrapper">
                                                <div class="swiper-slide">
                                                    <img src="{{ asset('assetsOfOutsiderElements/images/small/img-9.jpg') }}" alt=""
                                                         class="imgTrash"/>
                                                </div>
                                                <div class="swiper-slide">
                                                    <img src="{{ asset('assetsOfOutsiderElements/images/small/img-9.jpg') }}" alt=""
                                                         class="imgTrash"/>
                                                </div>
                                                <div class="swiper-slide">
                                                    <img src="{{ asset('assetsOfOutsiderElements/images/small/img-9.jpg') }}" alt=""
                                                         class="imgTrash"/>
                                                </div>
                                            </div>

                                            <div class="swiper-pagination"></div>
                                        </div>

                </a>

                <div class="rbt-quick-action--btns">
                    <div
                            class="quickview--box"
                            data-bs-toggle="tooltip"
                            data-bs-placement="right"
                            title="Quick View">
                        <a
                                href="#"
                                class="yith-wcqv-button"
                                data-product_id="793"
                        >
                            <i class="feather-search"></i>
                        </a>
                    </div>
                    <div
                            class="quickview--box"
                            data-bs-toggle="tooltip"
                            data-bs-placement="right"
                            title="Contact Seller">
                        <a
                                href="#"
                                class="yith-wcqv-button"
                                data-product_id="793"
                        >
                            <i class="mt-2 fi fi-rr-comment-alt"></i>
                        </a>
                    </div>
                    <div
                            class="quickview--box"
                            data-bs-toggle="tooltip"
                            data-bs-placement="right"
                            title="Buy"

                    >

                        <a
                                onclick="add2Card({{ product.idProduct }},{{ i+1 }})"
                                class="yith-wcqv-button"
                                id="product_{{ i+1 }}"
                        >
                                <i class="mt-2 fi fi-rr-shopping-cart-add"></i>
                        </a>

                    </div>
                </div>
            </div>
            <div class="product-share-wrapper">
                <div class="profile-share">
                    <a
                            href="author.html"
                            class="avatar"
                            data-tooltip="Owener:Mr.Jone-lee"
                    ><img
                                src="{{ asset('assets/images/client/client-1.png') }}"
                                alt="Nft_Profile"
                        /></a>
                    <a
                            href="author.html"
                            class="avatar"
                            data-tooltip="Owener:Mr.Jone-lee"
                    ><img
                                src="{{ asset('assets/images/client/client-10.png') }}"
                                alt="Nft_Profile"
                        /></a>
                    <a
                            href="author.html"
                            class="avatar"
                            data-tooltip="Owener:Mr.Jone-lee"
                    ><img
                                src="{{ asset('assets/images/client/client-11.png') }}"
                                alt="Nft_Profile"
                        /></a>
                    <a class="more-author-text" href="#">10+ Place Bit.</a>
                </div>
                <div class="share-btn share-btn-activation dropdown">
                    <button
                            class="icon"
                            type="button"
                            data-bs-toggle="dropdown"
                            aria-expanded="false"
                    >
                        <svg
                                viewBox="0 0 14 4"
                                fill="none"
                                width="16"
                                height="16"
                                class="sc-bdnxRM sc-hKFxyN hOiKLt"
                        >
                            <path
                                    fill-rule="evenodd"
                                    clip-rule="evenodd"
                                    d="M3.5 2C3.5 2.82843 2.82843 3.5 2 3.5C1.17157 3.5 0.5 2.82843 0.5 2C0.5 1.17157 1.17157 0.5 2 0.5C2.82843 0.5 3.5 1.17157 3.5 2ZM8.5 2C8.5 2.82843 7.82843 3.5 7 3.5C6.17157 3.5 5.5 2.82843 5.5 2C5.5 1.17157 6.17157 0.5 7 0.5C7.82843 0.5 8.5 1.17157 8.5 2ZM11.999 3.5C12.8274 3.5 13.499 2.82843 13.499 2C13.499 1.17157 12.8274 0.5 11.999 0.5C11.1706 0.5 10.499 1.17157 10.499 2C10.499 2.82843 11.1706 3.5 11.999 3.5Z"
                                    fill="currentColor"
                            ></path>
                        </svg>
                    </button>

                    <div
                            class="share-btn-setting dropdown-menu dropdown-menu-end"
                    >
                        <button
                                type="button"
                                class="btn-setting-text share-text"
                                data-bs-toggle="modal"
                                data-bs-target="#placebidModal"
                        >
                            Place Bid
                        </button>

                        <button
                                type="button"
                                class="btn-setting-text share-text"
                                data-bs-toggle="modal"
                                data-bs-target="#shareModal"
                        >
                            Share
                        </button>
                        <button
                                type="button"
                                class="btn-setting-text report-text"
                                data-bs-toggle="modal"
                                data-bs-target="#reportModal"
                        >
                            Report
                        </button>
                    </div>
                </div>
            </div>
            <a href="product-details.html.twig"
            >

                <span class="product-name">{{ product.name }}</span>
            </a
            >
            <span class="latest-bid">Highest bid 1/20</span>
            <div class="bid-react-area">
                <div class="last-bid">{{ product.price~" Dnt" }}</div>
                <div class="react-area">
                    <svg
                            viewBox="0 0 17 16"
                            fill="none"
                            width="16"
                            height="16"
                            class="sc-bdnxRM sc-hKFxyN kBvkOu"
                    >
                        <path
                                d="M8.2112 14L12.1056 9.69231L14.1853 7.39185C15.2497 6.21455 15.3683 4.46116 14.4723 3.15121V3.15121C13.3207 1.46757 10.9637 1.15351 9.41139 2.47685L8.2112 3.5L6.95566 2.42966C5.40738 1.10976 3.06841 1.3603 1.83482 2.97819V2.97819C0.777858 4.36443 0.885104 6.31329 2.08779 7.57518L8.2112 14Z"
                                stroke="currentColor"
                                stroke-width="2"
                        ></path>
                    </svg>
                    <span class="number">322</span>
                </div>
            </div>
        </div>
    </div>
    <!-- end single product -->

{% endfor %}





